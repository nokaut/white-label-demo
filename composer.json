{
    "name": "nokaut/white-label-demo",
    "description": "Price comparison site demo",
    "license": "proprietary",
    "type": "project",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:nokaut/api-kit.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-memcached": "*",
        "desarrolla2/cache": "3.0.0",
        "doctrine/annotations": "^1.0",
        "evert/sitemap-php": "~1.2",
        "incenteev/composer-parameter-handler": "~2.1",
        "nokaut/api-kit": "1.8.1",
        "symfony/asset": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/monolog-bundle": "*",
        "symfony/security-csrf": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.0",
        "mockery/mockery": "^1.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpunit/phpunit": "^9.6.5",
        "symfony/css-selector": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/maker-bundle": "*",
        "symfony/panther": "^1.1",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "incenteev-parameters": {
            "file": "config/packages/parameters.yml"
        },
        "branch-alias": null
    }
}
