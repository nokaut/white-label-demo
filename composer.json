{
    "name": "nokaut/white-label-demo",
    "description": "Price comparison site demo",
    "license": "proprietary",
    "type": "project",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:nokaut/api-kit.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-memcached": "*",
        "doctrine/annotations": "^1.0",
        "incenteev/composer-parameter-handler": "~2.1",
        "nokaut/api-kit": "1.8.1",
        "presta/sitemap-bundle": "^3.3",
        "symfony/asset": "6.2.*",
        "symfony/cache": "6.2.*",
        "symfony/config": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/dependency-injection": "6.2.*",
        "symfony/event-dispatcher": "6.2.*",
        "symfony/filesystem": "6.2.*",
        "symfony/flex": "2.*",
        "symfony/form": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-foundation": "6.2.*",
        "symfony/http-kernel": "6.2.*",
        "symfony/monolog-bundle": "*",
        "symfony/options-resolver": "6.2.*",
        "symfony/routing": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-csrf": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/validator": "6.2.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.0",
        "mockery/mockery": "^1.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpunit/phpunit": "^9.6.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "preferred-install": {
            "*": "dist"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "docker": true
        },
        "incenteev-parameters": {
            "file": "config/packages/parameters.yml"
        },
        "branch-alias": null
    }
}
